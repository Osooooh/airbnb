extends ../layout

block content
  .container
    h1.page-header
        b 회원 목록
    form(action='/users', method='POST')
        .form-group
            label(for='password') 관리자 비밀번호
            input.form-control(type='password', id='password', name='password', placeholder='Administrator Password')
        p
